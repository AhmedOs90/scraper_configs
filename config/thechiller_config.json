{
  "rootUrl": "https://thechiller.co.nz",
  "baseUrl": "https://thechiller.co.nz/collections/all",
  "site_name": "thechiller",
  "seller": "seller",
"config":true,
  "productsLinks": "/products/",
  "collectionLinks": "/collections/",
  "productLinkSelector": "a[href*=\"/products/\"]",
  "productLinkAttribute": "href",
  "skipCollectionLinksInProducts": true,
  "renderWaitMs": 7000,        
  "selectorWaitMs": 20000,    
  "minProductLinks": 12,        
  "linkPollAttempts": 8,        
  "linkPollIntervalMs": 700      ,
"pagination": {
  "type": "link",
  "selector": ".pagination a[aria-label='Next page'], nav[aria-label='Pagination'] a[aria-label*='Next']"
},

  "bannerSkip": null,
  "moreConfig": true,

  "selectors": {
    "header": {
      "name": "meta[property='og:title']",
      "price": "meta[property='product:price:amount'], meta[property='og:price:amount']",
      "currency": "meta[property='product:price:currency'], meta[property='og:price:currency']",
      "description": "meta[name='description'], meta[property='og:description']",
      "images": "meta[property='og:image']",
      "producer": "meta[property='product:brand']",
      "abv": null,
      "gluten_free": null,
      "vegan": null,
      "energy": null,
      "sugars": null,
      "category": null
    },
    "main": {
      "name": "h1.product__title, h1.product-title, h1",
      "price": ".price-item--regular, .price-item--sale, .price__current .money, [data-product-price]",
      "currency": ".price-item--regular, .price-item--sale, .price__current .money",
      "description": ".product__description, .product-single__description, .product-description, .rte, .product__accordion .rte",
      "images": ".product__media img, .product-gallery img, img[src*='/products/']",
      "producer": ".product__vendor, .product-meta__vendor a, .product-vendor",
      "abv": "li:contains('ABV'), li:contains('Alcohol'), p:contains('% ABV')",
      "gluten": "li:contains('Gluten'), .product__tags a:contains('Gluten')",
      "vegan": "li:contains('Vegan'), .product__tags a:contains('Vegan')",
      "energy": "li:contains('Calories'), p:contains('Calories')",
      "sugars": "li:contains('Sugar'), li:contains('Sugars'), li:contains('Carbohydrates'), p:contains('Sugar'), p:contains('Sugars')",
      "category": ".breadcrumb li:last-child, nav.breadcrumbs li:last-child, .breadcrumbs a:last-child, nav[aria-label='breadcrumbs'] a:last-child"
    }
  }
}
