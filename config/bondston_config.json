      "abv": "main .param-abv, main [itemprop*='alcohol'], main .product-alcohol, .param-abv",
      "gluten": "main .param-gluten, main [data-param='Gluten'], .param-gluten",
      "vegan": "main .param-vegan, main [data-param='Vegan'], .param-vegan",
      "producer": "main a[href*='-m'], .product a[href*='-m']",
      "energy": "main .param-energy, main [data-param='Energy'], .param-energy",
