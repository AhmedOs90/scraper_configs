{
    "baseUrl": "https://www.beyondbeer.de/beliebtes/alkoholfreies-craft-beer/",
    "rootUrl": "https://www.beyondbeer.de",
    "collectionLinks": "",
    "productsLinks": "pageLinks",
    "moreConfig": true,
    "seller": "seller",
    "bannerSkip": "button.klaviyo-close-form",
    "site_name": "beyondbeer",
    "skipCollectionLinksInProducts": true,
    "productLinkSelector": "a.product-tile__link",
    "productLinkAttribute": "href",
    "selectors": {
        "header": {
            "name": "meta[property='og:title']",
            "price": "meta[property='product:price:amount']",
            "currency": "meta[property='product:price:currency']",
            "producer": "meta[property='product:brand']",
            "description": "meta[property='og:description']",
            "images": "meta[property='og:image']"
        },
        "main": {
            "abv": ".product-info__abv-unit-size span:first-of-type",
            "price": ".price__current span:not(.visually-hidden)",
            "gluten": "div[data-tooltip='Gluten Free'] .sr-only",
            "vegan": "div[data-tooltip='Vegan'] .sr-only",
            "producer": ".product-info__vendor a, .product-info__vendor span"
        }
    },
    "pagination": {
        "type": "scroll",
        "selector": "div[x-intersect\\.full='fetchAndRenderNextPage()']"
    }
}