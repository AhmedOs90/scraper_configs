{
    "rootUrl": "https://softerdrink.ca",
    "baseUrl": "https://softerdrink.ca/collections/non-alcoholic-beers",
    "site_name": "softerdrink",
    "seller": "seller",
    "productsLinks": "/products/",
    "collectionLinks": "/collections/non-alcoholic",
    "productLinkSelector": "a[href*=\"/products/\"]",
    "productLinkAttribute": "href",
    "skipCollectionLinksInProducts": false,
    "level": 1,
    "moreConfig": true,
    "purePlayer": true,
    "localization": {
        "country": "CA",
        "language": "EN",
        "currency": "CAD"
    },
    "pagination": {
        "type": "link",
        "selector": "a[aria-label='Next page']"
    },
    "selectors": {
        "header": {
            "name": "meta[property='og:title']",
            "price": "meta[property='og:price:amount']",
            "currency": "meta[property='og:price:currency']",
            "description": "meta[name='twitter:description'], meta[property='og:description']",
            "images": "meta[property='og:image']",
            "producer": "meta[property='product:brand']"
        },
        "main": {
            "name": "h1.product__title, h1.product-title, h1",
            "currency": ".price__container .money",
            "description": ".product__description, .rte, .product-single__description",
            "images": ".product__media img, .product-gallery img, img[src*='/products/']",
            "producer": ".product__vendor a, .product-vendor a"
        }
    }
}