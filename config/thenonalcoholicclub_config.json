{
    "rootUrl": "https://thenonalcoholicclub.com",
    "baseUrl": "https://thenonalcoholicclub.com/collections/all",
    "site_name": "thenonalcoholicclub",
    "seller": "seller",
    "productsLinks": "/products/",
    "collectionLinks": "/collections/",
    "skipCollectionLinksInProducts": true,
    "moreConfig": true,
    "selectors": {
        "header": {
            "name": "meta[property='og:title']",
            "price": "meta[property='product:price:amount']",
            "currency": "meta[property='product:price:currency']",
            "images": "meta[property='og:image']"
        },
        "main": {
            "description": ".ProductMeta__Description"
        }
    },
    "pagination": {
        "type": "link",
        "selector": "a.Pagination__NavItem[rel=\"next\"]"
    },
    "purePlayer": true
}