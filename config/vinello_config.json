{
    "rootUrl": "https://www.vinello.dk",
    "baseUrl": "https://www.vinello.dk/alcfree",
    "site_name": "vinello",
    "seller": "seller",
    "bannerSkip": ".cookie-permission--accept-button",
    "productLinkAttribute": "href",
    "productLinkSelector": ".product--box a.product--image",
    "moreConfig": true,
    "selectors": {
        "header": {
            "name": "meta[property='og:title']",
            "description": "meta[property='og:description']",
            "images": "meta[name='twitter:image']",
            "producer": "meta[property='product:brand']",
            "price": "meta[property='product:price']"
        },
        "main": {
            "abv": ".product--properties-row #attribute-1"
        }
    },
    "pagination": {
        "type": "link",
        "selector": ".listing--bottom-paging .paging--link:first-of-type"
    },
    "purePlayer": true,
    "level": 1
}