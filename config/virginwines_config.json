{
    "rootUrl": "https://www.virginwines.co.uk",
    "baseUrl": "https://www.virginwines.co.uk/wines/non-alcoholic-wines",
    "site_name": "virginwines",
    "seller": "seller",
    "bannerSkip": "button.acceptallcookies",
    "productLinkAttribute": "href",
    "productLinkSelector": ".product-bottle .product-name a",
    "moreConfig": true,
    "selectors": {
        "header": {
            "name": "meta[property='og:title']",
            "description": "meta[property='og:description']",
            "images": "meta[property='og:image']"
        },
        "main": {
            "abv": "ul.main-attributes.attributes li:first-child span.font-weight-bold.large",
            "price": ".price",
            "producer": "a[data-gaaction='winemaker-info-click']",
            "vegan": "ul.other-classifications svg.classification.vegan + p"
        }
    },
    "pagination": {
        "type": "link",
        "selector": "a[aria-label=\"Next\"]"
    },
    "level": 1,
    "purePlayer": false
}