{
    "rootUrl": "https://heynolo.com",
    "baseUrl": "https://heynolo.com/collections/everything-na",
    "site_name": "heynolo",
    "seller": "seller",
    "bannerSkip": ".popup__wrapper .age-checker__actions button.age-checker__btn",
    "productsLinks": "/collections/everything-na/products/",
    "collectionLinks": "/collections/",
    "skipCollectionLinksInProducts": true,
    "moreConfig": true,
    "selectors": {
        "header": {
            "name": "meta[property='og:title']",
            "price": "meta[property='og:price:amount']",
            "currency": "meta[property='og:price:currency']",
            "images": "meta[property='og:image']"
        },
        "main": {
            "producer": ".product-single__vendor a",
            "description": ".product-single__box__block--description .rte"
        }
    },
    "pagination": {
        "type": "link",
        "selector": ".pagination .next a"
    },
    "level": 2
}