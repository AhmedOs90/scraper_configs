{
    "rootUrl": "https://flaskandfield.com",
    "baseUrl": "https://flaskandfield.com/collections/non-alcoholic-1",
    "site_name": "flaskandfield",
    "seller": "seller",
    "bannerSkip": "#submit_birthdate",
    "productsLinks": "/products/",
    "collectionLinks": "/collections/",
    "skipCollectionLinksInProducts": true,
    "moreConfig": true,
    "selectors": {
        "header": {
            "name": "meta[property='og:title']",
            "images": "meta[property='og:image']",
            "price": "meta[property='og:price:amount']",
            "currency": "meta[property='og:price:currency']"
        },
        "main": {
            "description": ".product__description"
        }
    },
    "pagination": {
        "type": "link",
        "selector": ".pagination__item:not(.pagination__item--current)"
    },
    "level": 1,
    "purePlayer": true
}