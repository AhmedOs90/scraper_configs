{
    "rootUrl": "https://boozefree.co.uk",
    "baseUrl": "https://boozefree.co.uk/collections/all-non-alcoholic-beers-and-ciders",
    "site_name": "boozefree",
    "seller": "seller",
    "productsLinks": "/products/",
    "collectionLinks": "/collections/",
    "skipCollectionLinksInProducts": true,
    "moreConfig": true,
    "selectors": {
        "header": {
            "name": "meta[property='og:title']",
            "price": "meta[property='og:price:amount']",
            "currency": "meta[property='og:price:currency']",
            "images": "meta[property='og:image']"
        },
        "main": {
            "producer": ".product__text",
            "description": ".product__description"
        }
    },
    "pagination": {
        "type": "link",
        "selector": ".pagination__item.pagination__item--prev"
    },
    "level": 2,
    "purePlayer": true
}