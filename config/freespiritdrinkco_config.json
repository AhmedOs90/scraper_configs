{
    "rootUrl": "https://freespiritdrinkco.com.au",
    "baseUrl": "https://freespiritdrinkco.com.au/collections/all",
    "site_name": "freespiritdrinkco",
    "seller": "seller",
    "productsLinks": "/collections/all/products/",
    "collectionLinks": "/products/",
    "skipCollectionLinksInProducts": true,
    "moreConfig": true,
    "selectors": {
        "header": {
            "name": "meta[property='og:title']",
            "price": "meta[property='og:price:amount']",
            "currency": "meta[property='og:price:currency']",
            "images": "meta[property='og:image']"
        },
        "main": {
            "producer": ".product-vendor a",
            "description": ".product-description"
        }
    },
    "pagination": {
        "type": "scroll",
        "selector": "a.js-pagination-load-more"
    },
    "level": 0,
    "purePlayer": true
}