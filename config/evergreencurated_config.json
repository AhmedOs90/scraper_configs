{
    "rootUrl": "https://evergreencurated.com",
    "baseUrl": "https://evergreencurated.com/collections/all-products",
    "site_name": "evergreencurated",
    "seller": "seller",
    "productsLinks": "/products/",
    "collectionLinks": "/collections/",
    "skipCollectionLinksInProducts": true,
    "moreConfig": true,
    "selectors": {
        "header": {
            "name": "meta[property='og:title']",
            "description": "meta[name='description']",
            "images": "meta[property='og:image']",
            "price": "meta[property='og:price:amount']",
            "currency": "meta[property='og:price:currency']"
        },
        "main": {}
    },
    "pagination": {
        "type": "link",
        "selector": ".pagination__item[aria-label=\"Next page\"]"
    },
    "level": 1,
    "purePlayer": true
}