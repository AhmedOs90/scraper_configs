{
    "rootUrl": "https://www.sechey.com",
    "baseUrl": "https://www.sechey.com/collections/all",
    "site_name": "sechey",
    "seller": "seller",
    "productsLinks": "/products/",
    "collectionLinks": "/collections/",
    "skipCollectionLinksInProducts": true,
    "moreConfig": true,
    "selectors": {
        "header": {
            "name": "meta[property='og:title']",
            "price": "meta[property='og:price:amount']",
            "currency": "meta[property='og:price:currency']",
            "images": "meta[property='og:image']"
        },
        "main": {
            "description": ".product-description"
        }
    },
    "pagination": {
        "type": "link",
        "selector": ".pagination-arrow.pagination-next > a"
    },
    "purePlayer": true
}