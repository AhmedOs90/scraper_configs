{
    "rootUrl": "https://beershoppen.dk",
    "baseUrl": "https://beershoppen.dk/collections/alkoholfri-ol",
    "site_name": "beershoppen",
    "seller": "seller",
    "productsLinks": "collections/alkoholfri-ol/products/",
    "collectionLinks": "collections/",
    "skipCollectionLinksInProducts": true,
    "moreConfig": true,
    "selectors": {
        "header": {
            "name": "meta[property='og:title']",
            "price": "meta[property='og:price:amount']",
            "currency": "meta[property='og:price:currency']",
            "description": "meta[name='description']",
            "images": "meta[property='og:image']"
        },
        "main": {
            "abv": ".product-info__abv-unit-size span:first-of-type",
            "gluten": "div[data-tooltip='Gluten Free'] .sr-only",
            "vegan": "div[data-tooltip='Vegan'] .sr-only",
            "producer": ".product-info__vendor a, .product-info__vendor span"
        }
    },
    "pagination": {
        "type": "link",
        "selector": "span.next a"
    },
    "purePlayer": false
}