{
    "rootUrl": "https://vesselliquor.com",
    "baseUrl": "https://vesselliquor.com/collections/non-alcoholic",
    "site_name": "vesselliquor",
    "seller": "seller",
    "productsLinks": "/products/",
    "collectionLinks": "/collections/",
    "skipCollectionLinksInProducts": true,
    "moreConfig": true,
    "selectors": {
        "header": {
            "name": "meta[property='og:title']",
            "price": "meta[property='product:price:amount']",
            "currency": "meta[property='product:price:currency']",
            "images": "meta[property='og:image']"
        },
        "main": {
            "description": "div.product-info__liquid #desc, .product-info__liquid > div > p > span"
        }
    },
    "pagination": {
        "type": "link",
        "selector": "nav.pagination a[rel=\"next\"]"
    },
    "level": 1,
    "purePlayer": false
}