{
    "rootUrl": "https://www.alcoholvrijshop.nl",
    "baseUrl": "https://www.alcoholvrijshop.nl/collections/all",
    "site_name": "alcoholvrijshop",
    "seller": "seller",
    "productsLinks": "alllinks",
    "collectionLinks": "/collections/",
    "moreConfig": true,
    "selectors": {
        "header": {
            "name": "meta[property='og:title']",
            "price": "meta[property='product:price:amount']",
            "currency": "meta[property='product:price:currency']",
            "description": "meta[name='description']",
            "images": "meta[property='og:image']"
        },
        "main": {
            "abv": "th:contains('Alcoholpercentage') + td",
            "energy": "th:contains('Energie') + td",
            "sugar": "th:contains('Suiker') + td",
            "gluten": "div[data-tooltip='Glutenvrij'] .sr-only",
            "vegan": "div[data-tooltip='Veganistisch'] .sr-only",
            "producer": ".product-meta__vendor a"
        }
    },
    "pagination": {
        "type": "link",
        "selector": "a.pagination__nav-item[rel='next']"
    },
    "purePlayer": false
}