{
    "rootUrl": "https://boisson.co",
    "baseUrl": "https://boisson.co/collections/all",
    "site_name": "boisson",
    "seller": "seller",
    "productsLinks": "/products/",
    "collectionLinks": "/collections/",
    "skipCollectionLinksInProducts": true,
    "moreConfig": true,
    "selectors": {
        "header": {
            "name": "meta[property='og:title']",
            "price": "meta[property='og:price:amount']",
            "currency": "meta[property='og:price:currency']",
            "images": "meta[property='og:image']"
        },
        "main": {
            "producer": ".product__form__link",
            "description": ".tab-content__entry"
        }
    },
    "pagination": {
        "type": "link",
        "selector": ".pagination-custom__arr a[title^='Next']"
    },
    "purePlayer": true
}