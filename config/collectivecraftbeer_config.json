{
    "rootUrl": "https://collectivecraftbeer.com",
    "baseUrl": [
        "https://collectivecraftbeer.com/collections/non-alcoholic-beer",
        "https://collectivecraftbeer.com/collections/non-alcoholic-wine",
        "https://collectivecraftbeer.com/collections/non-alcoholic-mixed-drinks"
    ],
    "site_name": "collectivecraftbeer",
    "seller": "seller",
    "bannerSkip": ".age-checker__btn.js-age-close",
    "productLinkAttribute": "href",
    "productLinkSelector": ".product-top > a.product-link.js-product-link",
    "moreConfig": true,
    "selectors": {
        "header": {
            "name": "meta[property='og:title']",
            "price": "meta[property='og:price:amount']",
            "currency": "meta[property='og:price:currency']",
            "images": "meta[property='og:image']"
        },
        "main": {
            "description": ".product-single__content-text.rte"
        }
    },
    "pagination": {
        "type": "link",
        "selector": ".pagination .next a"
    },
    "purePlayer": false,
    "level": 1
}