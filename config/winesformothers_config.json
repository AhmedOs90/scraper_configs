{
    "rootUrl": "https://winesformothers.com",
    "baseUrl": "https://winesformothers.com/shop-non-alcoholic-wines-for-moms/",
    "site_name": "winesformothers",
    "seller": "seller",

    "collectionLinks": "",
    "skipCollectionLinksInProducts": false,
    
    "productsLinks": "",
    "productLinkSelector": ".productGrid .card-title a, .productGrid h3 a",
    "productLinkAttribute": "href",
    
    "moreConfig": true,
    "selectors": {
        "header": {
            "name": "meta[property='og:title']",
            "category": "meta[property='og:type']",
            "price": "meta[property='product:price:amount'], meta[property='product:price']",
            "currency": "meta[property='product:price:currency']",
            "images": "meta[property='og:image']",
            "description": "meta[name='description']"
        },
        "main": {
            "producer": ".productView-brand a, .brand a"
        }
    },

    "pagination": {
        "type": "link",
        "selector": ".pagination-item--next .pagination-link"
    }
}
